    "@types/react-bootstrap": "0.0.32",
    "@types/react-dom": "^0.14.16",
    "@types/react-redux": "^4.4.31",
    "@types/react-router": "^2.0.33",
    "@types/react-router-redux": "^4.0.32",
    "@types/react-tap-event-plugin": "0.0.29",
    "@types/redux-form": "^4.0.29",
    "@types/redux-logger": "^2.6.31",
    "@types/redux-thunk": "^2.1.30",
    "@types/whatwg-fetch": "0.0.30",
    "awesome-typescript-loader": "^2.2.4",
    "babel-core": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "bootstrap": "^3.3.7",
    "concurrently": "^2.2.0",
    "core-js": "^2.4.1",
    "cross-env": "^2.0.1",
    "css-loader": "^0.24.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "immutable": "^3.8.1",
    "jquery": "^3.1.0",
    "node-sass": "^3.10.0",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-click-outside": "^2.1.0",
    "react-dom": "^15.3.1",
    "react-onclickout": "^2.0.4",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
