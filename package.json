{
  "name": "ts-react-redux-base",
  "version": "1.0.0",
  "description": "Base Boilerplate for React + Redux + TypeScript",
  "main": "index.js",
  "scripts": {
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info --port 8000",
    "dev:server": "cross-env NODE_ENV=development ls",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production  webpack -p",
    "dev": "concurrently -r \"npm run dev:client\" \"npm run dev:server\"",
    "clean": "rimraf dist/",
    "pretest": "npm run lint",
    "start": "cross-env NODE_ENV=production ls",
    "tslint": "tslint \"src/**/*.tsx\" \"src/**/*.ts\""
  },
  "author": "Eduardo Nunes Pereira",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^6.0.38",
